<?php

class AddChildTest extends \PHPUnit\Framework\TestCase
{
public function insertChild(){
$this->AddChild->Name('Rai');
$this->AddChild->ID('6');
$this->AddChild->Gender('male');
$this->AddChild->Age('8');
$this->AddChild->Area('Khulna');

$this->assertEquals($AddChild->Name(), 'Rai');
$this->assertEquals($AddChild->ID(), '6');
$this->assertEquals($AddChild->Gender(), 'male');
$this->assertEquals($AddChild->Age(), '8');
$this->assertEquals($AddChild->Area(), 'Khulna');
}
}
